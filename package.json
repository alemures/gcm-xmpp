{
  "name": "gcm-xmpp",
  "description": "Connects to GCM servers using a XMPP protocol.",
  "version": "1.0.4",
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "node-xmpp-client": "^3.2.0",
    "utjs": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "jscs": "^3.0.7",
    "jshint": "^2.9.6",
    "mocha": "^5.2.0"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/*.js",
    "jshint": "jshint index.js lib/*.js test/*.js",
    "jscs": "jscs index.js lib/*.js test/*.js",
